        <item name="actionBarWidgetTheme">@style/ActionBarWidgetTheme.Light</item>
        <item name="actionModeCloseButtonStyle">@style/ActionModeCloseButtonStyle.Light</item>
    </style>

    <style name="ActivityTheme.Base.Dark" parent="@style/Theme.Dark_ab">

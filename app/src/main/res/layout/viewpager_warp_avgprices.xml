<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin" >

    <TextView
        android:id="@+id/screen_warp_avgprices_name"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <TextView
        android:id="@+id/screen_warp_avgprices_resources"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginLeft="-1dp"
        android:layout_marginRight="-1dp"
        android:baselineAligned="false">
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="3.65"
            android:orientation="vertical">

            <RelativeLayout
                android:id="@+id/screen_warp_avgprices_water_layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingLeft="1dp"
                android:paddingRight="1dp"
                android:background="?avgPriceListBackground"
                android:onClick="pagerClick">
                <TextView
                    android:id="@+id/screen_warp_avgprices_water_label"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentLeft="true"
                    android:text="@string/tradeitem_water"
                    android:textColor="?pressableTextColor"/>
                <TextView
                    android:id="@+id/screen_warp_avgprices_water_price"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:textColor="?pressableTextColor" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/screen_warp_avgprices_furs_layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingLeft="1dp"
                android:paddingRight="1dp"
                android:background="?avgPriceListBackground"
                android:onClick="pagerClick">
                <TextView
                    android:id="@+id/screen_warp_avgprices_furs_label"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentLeft="true"
                    android:text="@string/tradeitem_furs"
                    android:textColor="?pressableTextColor" />
                <TextView
                    android:id="@+id/screen_warp_avgprices_furs_price"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:textColor="?pressableTextColor" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/screen_warp_avgprices_food_layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingLeft="1dp"
                android:paddingRight="1dp"
                android:background="?avgPriceListBackground"
                android:onClick="pagerClick">
                <TextView
                    android:id="@+id/screen_warp_avgprices_food_label"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentLeft="true"
                    android:text="@string/tradeitem_food"
                    android:textColor="?pressableTextColor" />
                <TextView
                    android:id="@+id/screen_warp_avgprices_food_price"

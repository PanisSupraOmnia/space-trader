                    android:layout_alignParentRight="true"
                    android:textColor="?pressableTextColor" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/screen_warp_avgprices_robots_layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingLeft="1dp"
                android:paddingRight="1dp"
                android:background="?avgPriceListBackground"
                android:onClick="pagerClick">
                <TextView
                    android:id="@+id/screen_warp_avgprices_robots_label"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentLeft="true"
                    android:text="@string/tradeitem_robots"
                    android:textColor="?pressableTextColor" />
                <TextView
                    android:id="@+id/screen_warp_avgprices_robots_price"
